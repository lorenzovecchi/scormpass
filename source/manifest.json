{
    "name": "Scormpass",
    "version": "0.0.1",
    "description": "Scormpass",
    "homepage_url": "https://elearning.unimib.it",
    "manifest_version": 3,
    "minimum_chrome_version": "100",
    "browser_specific_settings": {
        "gecko": {
            "id": "scormpass",
            "strict_min_version": "100.0"
        }
    },
    "icons": {
        "128": "icon.png"
    },
    "permissions": [],
    "host_permissions": [
        "https://elearning.unimib.it/*"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://elearning.unimib.it/mod/scorm/*"
            ],
            "js": [
                "content.js"
            ],
            "css": [
                "content.css"
            ],
            "run_at": "document_end"
        }
    ],
    "options_ui": {
        "browser_style": true,
        "page": "options.html"
    }
}
